    "N = int(input())\n",
    "\n",
    "def hanoi(n, start, end):\n",
    "    mid = 6-start-end\n",
    "    if n == 1:\n",
    "        print(start, end)\n",
    "    else:\n",
    "        hanoi(n-1, start, mid)\n",
    "        print(start, end)\n",
    "        hanoi(n-1, mid, end)\n",
    "\n",
    "print(2**N - 1)\n",
    "hanoi(N, 1, 3)"
   ]
